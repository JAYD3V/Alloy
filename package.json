{
   "name": "alloy-iconic",
   "description": "A VS Code extension that allows the user to dynamically configure there product theme icons.",
   "type": "module",
   "main": "./build/alloy.mjs",
   "types": "./types",
   "version": "0.0.0",
   "repository": "https://github.com/JAYD3V/Alloy.git",
   "author": "Andrew Chambers <W3Dojo@Gmail.com>",
   "license": "Apache-2.0",
   "scripts": {
      "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
      "build": "tsc",
      "entry-point": "node ./build/extension.mjs"
   },
   "engines": {
      "vscode": "*",
      "node": ">=14.0.0"
   },
   "contributes": {
      "productIconThemes": [
         {
            "id": "alloy-product-icon-theme",
            "label": "Alloy",
            "path": "./theme/alloy-iconic-product-icon-theme.json"
         }
      ]
   },
   "devDependencies": {
      "@jest/globals": "^28.1.1",
      "@types/jest": "^28.1.1",
      "@types/node": "^17.0.44",
      "@types/vscode": "^1.68.0",
      "@typescript-eslint/eslint-plugin": "^5.28.0",
      "@typescript-eslint/parser": "^5.28.0",
      "eslint": "^8.17.0",
      "jest": "^28.1.1",
      "prettier": "^2.7.0",
      "typescript": "^4.7.3",
      "vsce": "^2.9.1",
      "vscode": "^0.9.9"
   },
   "dependencies": {
      "webfont": "^11.2.26"
   }
}
