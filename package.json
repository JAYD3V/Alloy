{
   "name": "alloy-iconic",
   "description": "A VS Code Extension that adds support for dynamic product icon configurations",
   "type": "module",
   "main": "./build/alloy.mjs",
   "types": "./types",
   "version": "0.0.0",
   "repository": "https://github.com/JAYD3V/Alloy.git",
   "author": "Andrew Chambers <W3Dojo@Gmail.com>",
   "license": "Apache-2.0",
   "scripts": {
      "build": "tsc",
      "entry-point": "node ./build/extension.mjs"
   },
   "engines": {
      "vscode": "*",
      "node": ">=14.0.0"
   },
   "devDependencies": {
      "@types/node": "^17.0.42",
      "@types/vscode": "^1.68.0",
      "@typescript-eslint/eslint-plugin": "^5.27.1",
      "@typescript-eslint/parser": "^5.27.1",
      "eslint": "^8.17.0",
      "eslint-config-prettier": "^8.5.0",
      "eslint-plugin-prettier": "^4.0.0",
      "prettier": "^2.7.0",
      "typescript": "^4.8.0-dev.20220611",
      "vsce": "^2.9.1",
      "vscode": "^0.9.9"
   },
   "dependencies": {
      "webfont": "^11.2.26"
   },
   "contributes": {
      "productIconThemes": [
         {
            "id": "alloy-product-icon-theme",
            "label": "Alloy",
            "path": "./theme/alloy-iconic-product-icon-theme.json"
         }
      ]
   }
}
